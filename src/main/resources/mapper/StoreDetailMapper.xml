  <select id="find" parameterType="Map" resultType="com.blm.bean.StoreDetail">
   /* select s.storeid,s.userid,s.tel,s.email,s.storename,s.storeaddress,s.certurl,s.shopfronturl,s.area,s.isvalid
    from storedetail s,*/
    select * from storedetail
    <where>
      <if test="storename!=null and storename!='' ">
        and storename like #{storename}
      </if>
    </where>
    <if test="start!=null and size!=null">
      limit #{start},#{size}
    </if>
  </select>


  <select id="getTotal" parameterType="Map" resultType="Long">
    select count(*) from storedetail
    <where>
      <if test="storename!=null and storename!='' ">
        and storename like #{storename}
      </if>
    </where>
  </select>





  <update id="update" parameterType="com.blm.bean.StoreDetail">
    update storedetail
    <set>
      <if test="tel!=null and tel!='' ">
        tel=#{tel},
      </if>
      <if test="email!=null and email!='' ">
        email=#{email},
