  <select id="findAllById"  parameterType="String" resultMap="ResultMap">
    select c.foodid,c.storeid,f.foodid,f.foodname,f.price,f.picpath,f.description,s.storeid,
    s.storename,s.storeaddress,s.tel,s.email,s.certurl,s.area
    from checkfood c,fooddetail f,storedetail s
    where c.foodid=f.foodid and c.storeid=s.storeid and s.storeid=#{storeid}
  </select>

